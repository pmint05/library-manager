<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx"
  xmlns:fx="http://javafx.com/fxml"
  fx:controller="com.app.librarymanager.controllers.HomeController"
  prefHeight="400.0" prefWidth="600.0">

  <VBox alignment="CENTER" prefHeight="267.0" prefWidth="388.0" spacing="10.0">
    <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Button fx:id="loginButton" onAction="#onLoginButtonClick" text="Login"/>
    <Button fx:id="registerButton" onAction="#onRegisterButtonClick" text="Register"/>
  </VBox>
</AnchorPane>
